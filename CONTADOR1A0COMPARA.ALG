algoritmo "contador1a0compara"
var
   C: inteiro
inicio
   para C <- 1 ate 10 faca
      escreval(C)
   fimpara
   Escreval("------------------------------------")
   para C <- 1 ate 10 passo 2 faca
      Escreval(C)
   fimpara
   Escreval("------------------------------------")
   para C <- 10 ate 1 passo -1 faca
       Escreval(C)
   fimpara
fimalgoritmo